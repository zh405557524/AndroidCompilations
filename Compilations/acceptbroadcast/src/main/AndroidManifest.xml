<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.soul.acceptbroadcast"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <receiver
            android:name=".receiver.XWReceiver"
            android:enabled="true"
            android:exported="true">

            <intent-filter >

                <action android:name="com.soul.ccav.XWLB"/>
            </intent-filter>
        </receiver>

        <!--  -1000 ~ 1000 -->
        <receiver
            android:name=".receiver.orderly.ShengjiReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter

                android:priority="800">

                <action android:name="com.soul.guowuyuan.FFL"/>
            </intent-filter>
        </receiver>
        <receiver
            android:name=".receiver.orderly.ShijiReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter
                android:priority="600"
            >

                <action android:name="com.soul.guowuyuan.FFL"/>
            </intent-filter>
        </receiver>
        <receiver
            android:name=".receiver.orderly.xiagnjiReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter
                android:priority="400">

                <action android:name="com.soul.guowuyuan.FFL"/>
            </intent-filter>
        </receiver>
        <receiver
            android:name=".receiver.orderly.NongMingReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter android:priority="-1000">
                <action android:name="com.soul.guowuyuan.FFL"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>